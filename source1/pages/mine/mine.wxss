Page {
  background: #FFFFFF;
}

.under {
  position: fixed;
  z-index: 101;
  width: 100%;
  background: white;
  height: 25%;
  bottom: 0px;
}

.bg {
  background: rgb(8, 8, 8);
  opacity: 0.5;
  position: fixed;
  bottom: 0px;
  z-index: 100;
  width: 100%;
  
}

.bottom-pos {
  -webkit-transform: translateY(100%);
  transform: translateY(100%);
}
button::after{
  border: none;
  }
.topclose {
  position: fixed;
  background: white;
  z-index: 100;
  width: 100%;
}

.xufei {
  width: 160rpx;
  height: 60rpx;
  border-radius: 40rpx;
  background: #FDFDFE;
  color: #E02D2B;
  line-height: 60rpx;
  text-align: center;
}
.vip_img {
  width: 72rpx;
  height: 68rpx;
}